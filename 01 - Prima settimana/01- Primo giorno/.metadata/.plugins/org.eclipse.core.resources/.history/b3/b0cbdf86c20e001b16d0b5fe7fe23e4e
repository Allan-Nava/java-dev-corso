package main;

public class Main 
{
	public static void main(String[] args) 
	{
		// Stampa stringa "Ciao mondo!" 
		System.out.println("Ciao mondo!");
		
		// Stampo un numero
		System.out.println(1 + 2);
		
		// Posso concatenare string e numeri
		
		System.out.println("Il risultato di 2 + 2 é: " + 4 );
	}
}
